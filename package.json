{
  "name": "TS_Part2",
  "version": "1.0.0",
  "description": "typing skill",
  "main": "./src/API/crud.ts",
  "types": "./src/API/crud.ts",
  "scripts": {
    "test": "jest --detectOpenHandles --coverage",
    "lint": "prettier --write . && prettier --check . && eslint . --ext .js,.ts && tsc --noEmit",
    "lint:fix": "prettier --write . && eslint . --fix --ext .js,.ts",
    "start": "webpack serve --mode development",
    "build": "webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aleks164/TS_Part2.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Aleks164/TS_Part2/issues"
  },
  "homepage": "https://github.com/Aleks164/TS_Part2#readme",
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@webpack-cli/serve": "^1.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "handlebars-webpack-plugin": "^2.2.1",
    "husky": "^4.3.4",
    "jest": "^26.6.3",
    "jest-localstorage-mock": "^2.4.5",
    "lint-staged": "^10.5.3",
    "prettier": "2.2.1",
    "typescript": "^4.5.4",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^4.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js,*.ts": [
      "prettier --write",
      "eslint --cache --fix --ext .js,.ts"
    ],
    "*.{css,md}": "prettier --write"
  },
  "dependencies": {
    "firebase": "^9.6.6"
  },
  "jest": {
    "clearMocks": true,
    "coverageDirectory": "./coverage/",
    "collectCoverage": false,
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60
      }
    },
    "setupFiles": [
      "jest-localstorage-mock"
    ]
  }
}